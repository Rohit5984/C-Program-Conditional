#include <stdio.h>

void fibonacciSeries(int n);

int main() {
    int n;
    printf("Enter number of terms: ");
    scanf("%d", &n);

    fibonacciSeries(n);
    return 0;
}

void fibonacciSeries(int n) {
    int a = 0, b = 1, next;
    printf("Fibonacci Series: ");
    for (int i = 1; i <= n; i++) {
        printf("%d ", a);
        next = a + b;
        a = b;
        b = next;
    }
    printf("\n");
}

/* Example Output =>
   Enter number of terms: 7
   Fibonacci Series: 0 1 1 2 3 5 8
*/
