#include <stdio.h>

void findFactors(int num);

int main() {
    int num;
    printf("Enter a number: ");
    scanf("%d", &num);

    findFactors(num);
    return 0;
}

void findFactors(int num) {
    printf("Factors of %d are: ", num);
    for (int i = 1; i <= num; i++) {
        if (num % i == 0) {
            printf("%d ", i);
        }
    }
    printf("\n");
}

/* Example Output =>
   Enter a number: 12
   Factors of 12 are: 1 2 3 4 6 12
*/
