#include <stdio.h>

void checkVotingEligibility(int age);

int main() {
    int age;
    printf("Enter age: ");
    scanf("%d", &age);

    checkVotingEligibility(age);
    return 0;
}

void checkVotingEligibility(int age) {
    if (age >= 18)
        printf("You are eligible to vote.\n");
    else
        printf("You are not eligible to vote.\n");
}

/* Example Output =>
   Enter age: 20
   You are eligible to vote.
*/
